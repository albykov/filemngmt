        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fm_test"
        mc:Ignorable="d"
        Title="MainWindow" Height="316.667" Width="643.627">
    <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-2.41,-5.339" Height="150" Width="615">
